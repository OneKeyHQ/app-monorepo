+  if (props.style) {
+    const style = createOrReuseStyleNode(
+      props.style,
+      oldNode && oldNode._props.style
+    );
+    if (style) {
+      props = {
+        ...props,
+        style,
+      };
+    }
+  }
+  const config = sanitizeProps(props);
+  if (config === undefined) {
+    return undefined;
+  }
+  if (oldNode && deepEqual(config, oldNode._config)) {
+    return oldNode;
+  }
+  return new AnimatedProps(props, config, callback);
