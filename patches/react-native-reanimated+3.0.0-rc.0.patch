+    const proxy =
+      typeof Proxy === 'function'
+        ? new Proxy({}, proxyHandler)
+        : createEventObjectProxyPolyfill();
+    alwaysNodes.push(createAnimatedAlways(ev(proxy)));
+    traverse(proxy, []);
+  }
