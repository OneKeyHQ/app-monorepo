+  let props;
+  for (const key in inputProps) {
+    const value = inputProps[key];
+    if (value instanceof AnimatedNode && !(value instanceof AnimatedEvent)) {
+      if (props === undefined) {
+        props = {};
+      }
+      props[key] = value.__nodeID;
+    }
+  }
+  return props;
+}
+
