+import invariant from 'invariant';
+import AnimatedNode, { getCallID, setCallID } from './AnimatedNode';
+import AnimatedClock from './AnimatedClock';
+import { val } from '../val';
+
+export class AnimatedParam extends AnimatedNode {
+  argsStack = [];
+  _prevCallID;
