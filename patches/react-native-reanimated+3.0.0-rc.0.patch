+
+  result.push_back({"x", elements[0]});
+  result.push_back({"y", elements[1]});
+
+  result.push_back({"pageX", elements[2]});
+  result.push_back({"pageY", elements[3]});
+
+  result.push_back({"width", elements[4]});
