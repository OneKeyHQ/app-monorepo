{
  "name": "@onekeyhq/web",
  "version": "0.0.1",
  "main": "index.js",
  "description": "Multi-chain support for BTC/ETH/BNB/NEAR/Polygon/Solana/Avalanche/Fantom and others",
  "scripts": {
    "dev": "webpack serve",
    "start": "WEB_PORT=3000 ENABLE_ANALYZER= ENABLE_ANALYZER_HTML_REPORT= webpack serve",
    "build": "rm -rf ./web-build && ENABLE_ANALYZER=1 ENABLE_ANALYZER_HTML_REPORT=1 NODE_ENV=production webpack build && cp ./web-build/index.html ./web-build/404.html && sh ./postbuild.sh",
    "lint": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "stats": "NODE_ENV=production ENABLE_ANALYZER=1 webpack build"
  },
  "dependencies": {
    "@onekeyhq/components": "*",
    "@onekeyhq/kit": "*",
    "expo": "49.0.6",
    "firebase": "^9.8.2"
  },
  "private": true,
  "devDependencies": {
    "tamagui-loader": "^1.59.0",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-subresource-integrity": "^5.2.0-rc.1"
  }
}
